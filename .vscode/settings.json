{
  "files.associations": {
    "*.jsx": "javascriptreact",
    "*.tsx": "typescriptreact",
  },
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "git.ignoreLimitWarning": true,
  "css.validate": false,
  "scss.validate": false,
  "stylelint.enable": true
}